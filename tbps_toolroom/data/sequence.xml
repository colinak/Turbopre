<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">

    <record id="sequence_toolroom_assignment" model="ir.sequence">
      <field name="name">Toolroom Assignment</field>
      <field name="code">tr.stock.picking.assignment</field>
      <field name="prefix">ASSIGNED/</field>
      <field name="padding">5</field>
      <field name="company_id" eval="False"/>
    </record>
    
    <record id="sequence_toolroom_loans" model="ir.sequence">
      <field name="name">Toolroom Loans</field>
      <field name="code">tr.stock.picking.loans</field>
      <field name="prefix">LOAN/</field>
      <field name="padding">5</field>
      <field name="company_id" eval="False"/>
    </record>

    <record id="sequence_toolroom_returns" model="ir.sequence">
      <field name="name">Toolroom Returns</field>
      <field name="code">tr.stock.picking.returns</field>
      <field name="prefix">RETURN/</field>
      <field name="padding">5</field>
      <field name="company_id" eval="False"/>
    </record>

    <record id="sequence_toolroom_transfers" model="ir.sequence">
      <field name="name">Toolroom Transfers</field>
      <field name="code">tr.stock.picking.transfers</field>
      <field name="prefix">TRANSFER/</field>
      <field name="padding">5</field>
      <field name="company_id" eval="False"/>
    </record>

  </data>
</odoo>
